  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^16.1.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^3.1.3",
    "sequelize": "^5.22.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "sequelize-cli": "^5.5.1"
  },
  "keywords": []
}
