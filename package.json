{
  "name": "jwt-demo",
  "version": "1.0.0",
  "description": "Demo API to test setting JWT authentication",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "migrate": "sequelize db:migrate --url  sqlite://$(pwd)/dblogistica.sqlite",
    "unmigrate": "sequelize db:migrate:undo:all --url  sqlite://$(pwd)/dblogistica.sqlite",
    "seed": "sequelize db:seed:all --url  sqlite://$(pwd)/dblogistica.sqlite",
    "unseed": "sequelize db:seed:undo:all --url sqlite://$(pwd)/dblogistica.sqlite"
  },
